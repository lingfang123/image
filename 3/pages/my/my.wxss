}
.photo {
	width: 70px;
	height: 70px;
	border-radius: 90px;
}
.nickname {
	margin-top: 5rpx;
  font-size: 32rpx;
	text-align: center;
	color: #696969;
}
.address {
  margin-top: 5rpx;
  text-align: center;
  font-size: 28rpx;
  color: #fff;
}
.myorder {
  /**border: 1rpx solid violet;*/
  height: 200rpx;
}
.order_font view{
  margin-top: 10rpx;
  height: 70rpx;
}
.myorder1 {
  border-bottom: 1px solid #dadada;
}
.order_font {
  margin-left: 25rpx;
  height: 70rpx;
  font-size: 34rpx;
  color: black;
}
.row_x {
  display: flex;
  flex-direction: row;
  align-items: center;
  text-align: center;
}

.row_w {
  height: 120rpx;
  width: 25%;
  margin-top: 2rpx;
  font-size: 26rpx;
  /**border: 1rpx solid red;*/
}
.row_i image{
  width: 80rpx;
  height: 80rpx;
}

.weui_cell{
    position: relative;
    display: flex;
    -webkit-box-align: center;
    -ms-flex-align: center;
    align-items: center;
}

.hr{
  margin-bottom: 15rpx;
  width: 100%;
  height: 20rpx;
  background-color: #dadada;
}

.menu .menu_ul {
  height: 90rpx;
  width: 100%;
  display: flex;
  flex-direction: row;
  position: relative;
  border-bottom: 1rpx solid #dadada;
}
.menu image {
  display: flex;
  width: 55rpx;
  height: 55rpx;
  margin: 20rpx auto;
}

.menu_ul .menu_li_1{
  display: inline-block;
  width: 100rpx;
  height: 50rpx;
}

.menu_ul .menu_li_2{
  width: 300rpx;
  font-size: 32rpx;
  line-height: 100rpx;
}

.menu_ul .menu_li_3{
  width: 100rpx;
  right: 10rpx;
  height: 50rpx;
  position: absolute;
}
